html,body{height:100%;margin:0}body{display:flex;flex-direction:column;min-height:100vh;background-color:#f5f2ed}#footer,#content a,.nav-link{color:#ff8243;text-decoration:none}#footer *,#content a *,.nav-link *{color:#ff8243}#footer:hover,#content a:hover,.nav-link:hover{color:#ff8243;text-decoration:underline}.navbar-brand{color:#23282b;text-decoration:none}.navbar-brand *{color:#23282b}.navbar-brand:hover{color:#23282b;text-decoration:underline}#content table td,#content table th{border:1px solid #ccc;padding:6px 12px;text-align:left}*{font-family:Inconsolata;font-size:12pt;font-weight:300;line-height:1.3}.container{max-width:600px}.navbar-brand{font-weight:400}.nav-link{font-weight:400}#content{flex:1 0 auto}#content a{font-weight:400}#content p{margin-bottom:.6rem;font-weight:300}#content h1,#content h2,#content h3,#content h4,#content h5,#content h6{font-size:medium;font-weight:700;margin:2rem 0 .6rem}#content h1{font-size:1.6rem}#content h2{font-size:1.4rem}#content h3{font-size:1.2rem}#content h4{font-size:1rem}#content img{display:block;margin:2rem auto;max-width:100%}#content .avatar>img{border-radius:50%;float:right;margin:-8px 0 0 16px;height:140px;width:140px}#content ol{counter-reset:list;list-style:none;padding-left:0}#content ol>li{display:table-row}#content ol>li:before{content:"[" counter(list,decimal)"] ";counter-increment:list;display:table-cell;text-align:right;padding-right:.5em}#content .container>ol,#content .footnotes>ol{padding-left:0}#content ul{list-style:inside;padding-left:2rem}#content ul>li{list-style-position:outside;margin-left:1em}#content .container>ul,#content .footnotes>ul{padding-left:0}#content table{margin:2rem auto;width:100%}#content table th{font-weight:700}#content table tr:nth-child(2n){background-color:#f8f8f8}#content blockquote{border-left:4px solid;font-style:italic;margin:2rem 0;padding:8px}#content code{color:#23282b;background-color:#f8f8f8;border:1px solid #ccc;border-radius:10%;padding:0 4px}#content pre code{all:unset}#content .highlight{margin:2rem auto}#content .highlight>pre{padding:8px}#footer{flex-shrink:0}