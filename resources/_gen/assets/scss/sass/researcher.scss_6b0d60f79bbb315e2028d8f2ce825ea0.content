#content a,.nav-link{color:#23282b;font-size:16px;font-weight:400;text-decoration:underline;text-decoration-color:#ff8243;text-underline-offset:.2em}#content a *,.nav-link *{color:#ff8243}#content a:hover,.nav-link:hover{color:#ff8243;text-decoration:underline}#footer a,.navbar-brand{color:#23282b;text-decoration:none}#footer a *,.navbar-brand *{color:#23282b}#footer a:hover,.navbar-brand:hover{color:#23282b;text-decoration:underline}#content table td,#content table th{border:1px solid #ccc;padding:6px 12px;text-align:left}*{color:#23282b;font-family:inconsolata;line-height:1.4;letter-spacing:.01rem;font-size:16px;font-weight:300;text-rendering:optimizeLegibility}h2{font-size:22px;font-weight:700;font-color:grey;padding-top:2em;padding-bottom:0;line-height:1em}.container{max-width:600px}.navbar-brand{font-size:20px;font-color:#23282b;font-family:inconsolata;text-transform:uppercase;font-weight:300px;letter-spacing:.2rem}#content p{margin-bottom:.6rem}#content h1,#content h2,#content h3,#content h4,#content h5,#content h6{margin:1rem 0 .6rem}#content img{display:block;margin:1rem auto;max-width:100%}#content .avatar>img{border-radius:50%;float:right;margin:-8px 0 0 16px;height:140px;width:140px}#content ol{counter-reset:list;list-style:none;padding-left:2rem}#content ol>li{display:table-row}#content ol>li:before{content:"[" counter(list,decimal)"] ";counter-increment:list;display:table-cell;text-align:right;padding-right:.5em}#content .container>ol,#content .footnotes>ol{padding-left:0}#content ul{list-style:inside;padding-left:2rem}#content ul>li{list-style-position:outside;margin-left:1em}#content .container>ul,#content .footnotes>ul{padding-left:0}#content table{margin:1rem auto;width:100%}#content table th{font-weight:700}#content table tr:nth-child(2n){background-color:#f8f8f8}#content blockquote{border-left:4px solid;font-style:italic;margin:1rem 0;padding:8px}#content code{color:#23282b;background-color:#eae5e0;border:1px solid #ccc;border-radius:10%;padding:0 4px}#content pre code{all:unset}#content .highlight{margin:1rem auto}#content .highlight>pre{padding:8px}